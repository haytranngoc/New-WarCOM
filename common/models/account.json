{
  "name": "Account",
  "plural": "accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false
    },
    "role_name": {
      "type": "string",
      "required": true
    },
    "phone_number": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "account_id"
    },
    "voted_posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "account_id",
      "through": "UserVote"
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "hidden": [
    "role_name",
    "realm",
    "username",
    "emailVerified"
  ]
}
