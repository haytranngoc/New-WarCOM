    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "type_id": {
      "type": "any",
      "required": true
    },
    "latlng": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "account_id": {
      "type": "any",
      "required": true
    }
